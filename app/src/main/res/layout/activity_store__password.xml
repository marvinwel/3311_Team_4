<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/store_password"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Store_Password">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="130dp"
        android:layout_height="61dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="17dp"
        android:layout_marginRight="17dp"
        android:layout_marginBottom="13dp"
        android:onClick="hideKeyboard"
        app:layout_constraintBottom_toTopOf="@+id/listview"
        app:layout_constraintEnd_toStartOf="@+id/addToList"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Password_text"
        app:layout_constraintVertical_bias="0.277" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="26dp"
        android:layout_height="200dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="88dp"
        android:layout_marginBottom="33dp"
        android:onClick="hideKeyboard"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintEnd_toStartOf="@+id/domain_name"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.72" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="128dp"
        android:layout_height="67dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="15dp"
        android:onClick="hideKeyboard"
        app:layout_constraintBottom_toTopOf="@+id/listview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/addToList"
        app:layout_constraintTop_toBottomOf="@+id/Password_text" />

    <ImageButton
        android:id="@+id/storepsswrd_back_bttn"
        android:layout_width="38dp"
        android:layout_height="35dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="9dp"
        android:background="@drawable/back_arrow"
        app:layout_constraintBottom_toTopOf="@+id/domain_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/Password_text"
        android:layout_width="350dp"
        android:layout_height="41dp"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.442"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Text_username"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/domain_name"
        android:layout_width="356dp"
        android:layout_height="41dp"
        android:layout_marginTop="60dp"
        android:drawableLeft="@drawable/web"
        android:ems="10"
        android:hint="Domain eg www.facebook.com"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/Text_username"
        android:layout_width="356dp"
        android:layout_height="41dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="UserName"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/domain_name" />

    <ImageView
        android:id="@+id/show_password_img"
        android:layout_width="wrap_content"
        android:layout_height="41dp"
        android:layout_marginTop="152dp"
        android:src="@drawable/show_password"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/Password_text"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/listview"
        android:layout_width="382dp"
        android:layout_height="450dp"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addToList" />

    <Button
        android:id="@+id/addToList"
        android:layout_width="151dp"
        android:layout_height="41dp"
        android:layout_marginTop="12dp"
        android:text="ADD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Password_text" />

</androidx.constraintlayout.widget.ConstraintLayout>